{
    "name": "fabcar",
    "version": "1.0.0",
    "description": "FabCar application implemented in JavaScript",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "start": "node ./bin/www",
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "bcrypt": "^3.0.7",
        "body-parser": "^1.19.0",
        "cookie-parser": "~1.4.4",
        "csurf": "^1.10.0",
        "debug": "~2.6.9",
        "espress": "0.0.0",
        "express": "~4.16.1",
        "express-handlebars": "^3.1.0",
        "express-session": "^1.17.0",
        "express-validator": "^6.3.1",
        "fabric-ca-client": "~1.4.0",
        "fabric-network": "~1.4.0",
        "http-errors": "~1.6.3",
        "lodash.groupby": "^4.6.0",
        "morgan": "^1.9.1",
        "multer": "^1.4.2",
        "path": "^0.12.7",
        "serve-favicon": "^2.5.0"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "eslint": "^5.9.0",
        "mocha": "^5.2.0",
        "nyc": "^13.1.0",
        "sinon": "^7.1.1",
        "sinon-chai": "^3.3.0"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
